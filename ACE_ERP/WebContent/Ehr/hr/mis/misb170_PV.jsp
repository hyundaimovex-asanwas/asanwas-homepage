    String acc_cd   = request.getParameter("acc_cd");
    String gbn_case = request.getParameter("gbn_case");
    String OCC_CD	= request.getParameter("OCC_CD");
       
    String page_val = "";
    if (gbn_case.equals("H")){
        page_val = "Page1";
