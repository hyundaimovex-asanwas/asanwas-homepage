<%@ include file="/common/sessionCheck.jsp" %>
<%
    String pmt_ymd   = request.getParameter("pmt_ymd");
    String jobdpt_cd = request.getParameter("jobdpt_cd");
    String jobdpt_val = "";
        // 직위, 부서
    if ( jobdpt_cd.equals("1") ) {
        String job_cd    = request.getParameter("job_cd");
        if ( job_cd.equals(null) || job_cd.equals("") ) {
            jobdpt_val = "";
        } else {
            jobdpt_val = "AND JOB_CD = '"+job_cd+"'";
        }
    } else {
        String dpt_cd    = request.getParameter("dpt_cd");
        if ( dpt_cd.equals(null) || dpt_cd.equals("") ) {
            jobdpt_val = "";
        } else {
            jobdpt_val = "AND DPT_CD = '"+dpt_cd+"'";
        }
    }

    String gbn_cd    = request.getParameter("gbn_cd");
    String toe_cd    = request.getParameter("toe_cd");
    String rec_cd    = request.getParameter("rec_cd");
    String lan_cd    = request.getParameter("lan_cd");

    String title_val = "";

    if ( gbn_cd.equals("1") ) {
        title_val = pmt_ymd + "년 정기승진 대상자 선정";
    } else {
        title_val = pmt_ymd + "년 특별승진 대상자 선정";
    }
    
%>
