///////////////////////////////////////////////////////////////////////////////
// Description	:  행옮김
// Parameter 	: 
%>
		function fnMove(p) {
				if (p=='L'){
					ds2.addrow();
					ds2.namevalue(ds2.rowposition,"project_level")=ds1.namevalue(ds1.rowposition,"project_level");
					ds2.namevalue(ds2.rowposition,"program_id")=ds3.namevalue(ds3.rowposition,"program_id");
					ds2.namevalue(ds2.rowposition,"program_nm")=ds3.namevalue(ds3.rowposition,"program_nm");
					ds3.deleterow(ds3.rowposition);
				}else if(p=='R'){
					ds3.addrow();
					ds3.namevalue(ds3.rowposition,"project_level")=ds1.namevalue(ds1.rowposition,"project_level");
					ds3.namevalue(ds3.rowposition,"program_id")=ds2.namevalue(ds2.rowposition,"program_id");
					ds3.namevalue(ds3.rowposition,"program_nm")=ds2.namevalue(ds2.rowposition,"program_nm");
					ds2.deleterow(ds2.rowposition);
				}else if(p=='LA'){
					var row=ds3.countrow;
					for(i=1;i<=row;i++) {
						ds2.addrow();
						ds2.namevalue(ds2.rowposition,"project_level")=ds1.namevalue(ds1.rowposition,"project_level");
						ds2.namevalue(ds2.rowposition,"program_id")=ds3.namevalue(i,"program_id");
						ds2.namevalue(ds2.rowposition,"program_nm")=ds3.namevalue(i,"program_nm");
					}
					ds3.deleteall();
				}else if(p=='RA'){
					var row=ds2.countrow;
					for(i=1;i<=row;i++) {
						ds3.addrow();
						ds3.namevalue(ds3.rowposition,"project_level")=ds1.namevalue(ds1.rowposition,"project_level");
						ds3.namevalue(ds3.rowposition,"program_id")=ds2.namevalue(i,"program_id");
						ds3.namevalue(ds3.rowposition,"program_nm")=ds2.namevalue(i,"program_nm");
					}
					ds2.deleteall();
				}
		}

