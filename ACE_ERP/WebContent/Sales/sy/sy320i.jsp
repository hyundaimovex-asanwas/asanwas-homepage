		function fnChk() {
				var row1=ds1.countrow;
				for(i=1;i<=row1;i++) {
					if(ds1.namevalue(i,"project_id")=="") {
						alert("프로젝트ID<%=HDConstant.A_MSG_MUST_INPUT%>");
						return false;
					}
				}
				for(i=1;i<row1;i++) {
					if(ds1.namevalue(row1,"project_id")==ds1.namevalue(i,"project_id")) {
						alert("프로젝트ID<%=HDConstant.A_MSG_MUST_INPUT%>");
						return false;
					}
				}
				return true;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
		}

